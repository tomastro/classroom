{
    "manifest_version": 3,
    "name": "Classroom Extension",
    "version": "1.0",
    "description": "Extension for Google Classroom",
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "https://classroom.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://classroom.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}